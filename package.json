{
  "name": "sleek0",
  "productName": "Sleek0",
  "version": "2.0.22",
  "description": "A minimal todo.txt manager for macOS",
  "synopsis": "A minimal todo.txt manager for macOS",
  "keywords": [
    "todo.txt",
    "todo",
    "productivity",
    "tasks",
    "macos"
  ],
  "category": "ProjectManagement",
  "license": "MIT",
  "author": {
    "name": "Robin Ahle",
    "email": "sleek@datenkrake.eu"
  },
  "copyright": "Copyright Â© 2021 to present Robin Ahle",
  "main": "./out/main/index.js",
  "type": "module",
  "node": ">=22",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npx typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "npm run peggy && electron-vite build",
    "build:mac": "npm run build && electron-builder build -m --publish never",
    "build:mas": "npm run build && electron-builder build -m mas --universal --publish never",
    "build:masdev": "npm run build && electron-builder build -m mas-dev --publish never",
    "postinstall": "electron-builder install-app-deps",
    "peggy": "npx peggy --format es --dts --output ./src/main/Filters/FilterLang.ts ./src/main/Filters/FilterLang.pegjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/system": "^7.2.0",
    "@mui/x-date-pickers": "^8.16.0",
    "chokidar": "^5.0.0",
    "dayjs": "^1.11.13",
    "electron-store": "^11.0.2",
    "i18next": "^25.3.2",
    "jstodotxt": "1.0.0-alpha.3",
    "luxon": "^3.7.1",
    "react": "^19.1.0",
    "react-autosuggest": "^10.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^16.0.0",
    "react-markdown": "^10.1.0",
    "react-string-replace": "^1.1.1",
    "remark-gfm": "^4.0.1",
    "sugar": "^2.0.6"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.0",
    "material-ui-popup-state": "^5.3.6",
    "peggy": "^5.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vitest": "^4.0.14"
  }
}
